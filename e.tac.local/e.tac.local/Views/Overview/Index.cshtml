@using e.tac.local.Models
@model OverviewList
    	<!-- [Category List] -->
<ul class="block-grid-md-4 block-grid-sm-3 block-grid-xs-1">
   @foreach (OverviewItem i in Model)
   {
     <li>
        <div class="thumbnail">
            
            <a href="@i.Url">
                @i.Image
            </a>
            <div class="caption">
                <h3 class="teaser-heading">Climbing</h3>
                <a href="@i.Url" class="btn btn-default">@Model.ReadMore</a>
            </div>
            
        </div>
    </li>
   }
</ul>
<!-- [Category List] -->
